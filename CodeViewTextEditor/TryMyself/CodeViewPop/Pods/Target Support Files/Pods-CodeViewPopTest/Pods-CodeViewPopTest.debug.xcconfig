GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
HEADER_SEARCH_PATHS = $(inherited) "${PODS_ROOT}/Headers/Public" "${PODS_ROOT}/Headers/Public/WEPopover" "${PODS_ROOT}/Headers/Public/iOS-Rich-Text-Editor"
LIBRARY_SEARCH_PATHS = $(inherited) "$PODS_CONFIGURATION_BUILD_DIR/WEPopover" "$PODS_CONFIGURATION_BUILD_DIR/iOS-Rich-Text-Editor"
OTHER_CFLAGS = $(inherited) -isystem "${PODS_ROOT}/Headers/Public" -isystem "${PODS_ROOT}/Headers/Public/WEPopover" -isystem "${PODS_ROOT}/Headers/Public/iOS-Rich-Text-Editor"
OTHER_LDFLAGS = $(inherited) -ObjC -l"WEPopover" -l"iOS-Rich-Text-Editor" -framework "Foundation" -framework "UIKit"
PODS_BUILD_DIR = $BUILD_DIR
PODS_CONFIGURATION_BUILD_DIR = $PODS_BUILD_DIR/$(CONFIGURATION)$(EFFECTIVE_PLATFORM_NAME)
PODS_ROOT = ${SRCROOT}/Pods
